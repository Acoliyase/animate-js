{
  "name": "@wix/unidriver-common",
  "description": "UniDrivers base classes and tests",
  "version": "1.4.7",
  "license": "UNLICENSED",
  "author": {
    "name": "Iaroslav Shvets",
    "email": "iaroslavs@wix.com"
  },
  "homepage": "https://github.com/wix-private/unidriver/tree/master/packages/unidriver-common",
  "exports": {
    ".": {
      "types": "./dist/types/exports.d.ts",
      "import": "./dist/esm/exports.js",
      "require": "./dist/cjs/exports.js"
    },
    "./testing": {
      "types": "./dist/types/exports-testing.d.ts",
      "import": "./dist/esm/exports-testing.js",
      "require": "./dist/cjs/exports-testing.js"
    }
  },
  "types": "dist/types/exports.d.ts",
  "main": "dist/cjs/exports.js",
  "module": "dist/esm/exports.js",
  "sideEffects": false,
  "files": [
    "testing",
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && yarn build:ts && mkdir dist/statics && echo \"{}\" > dist/statics/dummy.json",
    "build:ts": "tsc --declarationDir dist/types && tsc --module CommonJS --outDir dist/cjs",
    "lint": "eslint ./src --max-warnings=0",
    "lint:fix": "yarn lint --fix",
    "test": "vitest run --typecheck",
    "test:watch": "vitest watch --typecheck",
    "ts": "tsc --noEmit"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress",
      "artifactId": "unidriver-common"
    },
    "validations": {
      "postDependenciesBuild": [
        "lint",
        "test"
      ]
    }
  },
  "dependencies": {
    "@wix/unidriver-core": "^1.4.7"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.37.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "falconPackageHash": "38d7f81592523f4871c9d94bdf6230d782ce2051d4f2a5443f37a85c"
}