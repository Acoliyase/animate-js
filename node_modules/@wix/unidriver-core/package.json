{
  "name": "@wix/unidriver-core",
  "description": "Utils for UniDrivers",
  "version": "1.4.7",
  "license": "UNLICENSED",
  "author": {
    "name": "Mykolas Å indeikis",
    "email": "mykolass@wix.com"
  },
  "homepage": "https://github.com/wix-private/unidriver/tree/master/packages/unidriver-core",
  "exports": {
    ".": {
      "types": "./dist/types/exports.d.ts",
      "import": "./dist/esm/exports.js",
      "require": "./dist/cjs/exports.js"
    },
    "./internal": {
      "types": "./dist/types/exports-internal.d.ts",
      "import": "./dist/esm/exports-internal.js",
      "require": "./dist/cjs/exports-internal.js"
    }
  },
  "types": "dist/types/exports.d.ts",
  "main": "dist/cjs/exports.js",
  "module": "dist/esm/exports.js",
  "sideEffects": false,
  "files": [
    "internal",
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && yarn build:ts && mkdir dist/statics && echo \"{}\" > dist/statics/dummy.json",
    "build:ts": "tsc --declarationDir dist/types && tsc --module CommonJS --outDir dist/cjs",
    "lint": "eslint ./src --max-warnings=0",
    "lint:fix": "yarn lint --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "ts": "tsc --noEmit"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress",
      "artifactId": "unidriver-core"
    },
    "validations": {
      "postDependenciesBuild": [
        "lint",
        "test"
      ]
    }
  },
  "devDependencies": {
    "eslint": "^9.37.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "falconPackageHash": "3311c54227e942fcee5d7cded9701b31467f42a6d9f38552a4b38f95"
}