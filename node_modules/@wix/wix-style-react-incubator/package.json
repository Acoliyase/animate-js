    "name": "Alexandr Yushkov",
    "email": "alexandry@wix.com"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "drag-and-drop",
    "dnd-kit",
    "styles.global.css"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "yarn stc:all && yoshi-library build",
    "start": "yarn stc:all && yoshi-library start",
    "storybook": "start-storybook -p 6006 -c ./.storybook/e2e",
    "test:unit": "yoshi-library test",
    "test:e2e": "build-storybook -c ./.storybook/e2e --output-dir storybook-e2e-static && npx sled-test-runner remote --config sled.json",
    "test": "npm run test:unit",
    "lint": "yoshi-library lint",
    "stc:all": "yarn stc:bundle && cp src/styles.global.css styles.global.css && yarn stc:src && yarn stc:ts",
    "stc:bundle": "stc --srcDir=./src --outDir=src --bundle=styles.global.css --minify --optimize --dm=loose",
    "stc:src": "stc --srcDir=./src --outDir=src --esm --esmExt=.js --optimize --inlineRuntime --dm=loose",
    "stc:ts": "ts-node scripts/rename-st-css-files.ts"
  },
  "dependencies": {
    "@dnd-kit/accessibility": "^3.1.1",
    "@dnd-kit/core": "https://wixmp-63fb97685840e8c34a920619.wixmp.com/npm/dnd-kit-core-6.1.0-wix.9.tgz",
    "@dnd-kit/modifiers": "https://wixmp-63fb97685840e8c34a920619.wixmp.com/npm/dnd-kit-modifiers-6.1.0-wix.7.tgz",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.26.0",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/core": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@stylable/cli": "^5.19.0",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.3.16",
    "@types/react-dom": "^18.3.5",
    "@wix/design-system": "^1.163.0",
    "@wix/jest-yoshi-preset-base": "^6.125.0",
    "@wix/sled-test-runner": "^2.0.445",
    "@wix/unidriver-core": "^1.1.8",
    "@wix/wix-ui-icons-common": "^3.66.0",
    "@wix/wix-ui-test-utils": "^1.1.16",
    "@wix/yoshi-flow-library": "^6.125.0",
    "@wix/yoshi-style-dependencies": "^6.125.0",
    "globby": "^11.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "wix-storybook-config": "^1.0.2"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.20.6",
    "@wix/design-system": "^1.163.0",
    "@wix/wix-ui-icons-common": "^3.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "jest": {
    "preset": "@wix/jest-yoshi-preset-base",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "yoshiFlowLibrary": {
    "buildEsmWithBabel": true
  },
  "eslintConfig": {
    "extends": "@wix/eslint-config-yoshi"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress",
