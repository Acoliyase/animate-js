{
  "name": "@wix/unidriver-puppeteer",
  "version": "1.4.7",
  "license": "UNLICENSED",
  "author": {
    "name": "Iaroslav Shvets",
    "email": "iaroslavs@wix.com"
  },
  "homepage": "https://github.com/wix-private/unidriver/tree/master/packages/unidriver-puppeteer",
  "exports": {
    "types": "./dist/types/exports.d.ts",
    "import": "./dist/esm/exports.js",
    "require": "./dist/cjs/exports.js"
  },
  "types": "dist/types/exports.d.ts",
  "main": "dist/cjs/exports.js",
  "module": "dist/esm/exports.js",
  "sideEffects": false,
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/types"
  ],
  "scripts": {
    "build": "yarn build:app && yarn build:sb",
    "build:app": "rm -rf dist && yarn build:ts && mkdir dist/statics && echo \"{}\" > dist/statics/dummy.json",
    "build:sb": "storybook build",
    "build:ts": "tsc --declarationDir dist/types && tsc --module CommonJS --outDir dist/cjs",
    "lint": "eslint ./src --max-warnings=0",
    "lint:fix": "yarn lint --fix",
    "start:sb": "yarn storybook dev -p 6006 --no-open",
    "start:sb:local": "storybook dev -p 6006",
    "test": "yarn start-server-and-test start:sb http://localhost:6006 test:runner",
    "test:local": "yarn start-server-and-test start:sb:local http://localhost:6006 test:runner",
    "test:runner": "vitest run --testTimeout=15000",
    "test:runner:debug": "DEBUG=true vitest run --testTimeout=150000",
    "ts": "tsc --noEmit"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress",
      "artifactId": "unidriver-puppeteer"
    },
    "validations": {
      "postDependenciesBuild": [
        "lint"
      ],
      "postBuild": [
        "test"
      ]
    }
  },
  "dependencies": {
    "@wix/unidriver-common": "^1.4.7",
    "@wix/unidriver-core": "^1.4.7",
    "array.prototype.at": "^1.1.3"
  },
  "devDependencies": {
    "@storybook/react-vite": "^9.1.10",
    "@types/node": "^22.18.8",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "ci-info": "^4.3.1",
    "eslint": "^9.37.0",
    "eslint-plugin-storybook": "^9.1.10",
    "puppeteer": "14.3.0",
    "puppeteer-core": "14.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "start-server-and-test": "^2.1.2",
    "storybook": "^9.1.10",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "falconPackageHash": "7018e8dcb5c3e5f03f18b4622a0eae421318f2bbda3c9c743f66e268"
}