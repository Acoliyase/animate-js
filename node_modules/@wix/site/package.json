    "access": "public"
  },
  "publishScoped": true,
  "scripts": {
    "build": "tsc && tsc --project tsconfig.cjs.json",
    "clean": "rm -rf dist build",
    "test": "vitest",
    "lint": "eslint --max-warnings=0 .",
    "lint:fix": "eslint --max-warnings=0 . --fix",
    "typecheck": "tsc --noEmit"
  },
