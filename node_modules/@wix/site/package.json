  },
  "devDependencies": {
    "@types/chance": "^1.1.6",
    "@types/node": "^20.19.22",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/ui": "^1.6.1",
    "@wix/essentials": "^0.1.14",
