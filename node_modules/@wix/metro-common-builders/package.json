{
  "name": "@wix/metro-common-builders",
  "version": "1.0.1676",
  "license": "UNLICENSED",
  "author": {
    "name": "Eli Ponyatovski",
    "email": "elip@wix.com"
  },
  "main": "dist/src/index.js",
  "module": "dist/es/src/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "start": "yoshi start",
    "build": "npm run build:proto && yoshi build && npm run copy:proto",
    "copy:proto": "cp -r src/proto dist/src && cp -r src/proto dist/es/src",
    "build:proto": "DEBUG=wix:* wix-proto-codegen --module . --dir src/proto --filename index --stringEnums --withAspectStore false --tsClasses --tsExportDefault --stringLongs",
    "test": "yoshi test",
    "lint": "yoshi lint",
    "posttest": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "yoshi lint"
  },
  "dependencies": {
    "@wix/metro-runtime": "^1.2031.0",
    "long": "^5.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^16.11.7",
    "@wix/business-schema-api": "^1.7.1630",
    "@wix/wix-proto-codegen": "^2.0.1170",
    "@wix/yoshi": "^6.95.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "~4.2.5",
    "lint-staged": "^10.5.4",
    "ts-jest": "^29.1.1",
    "typescript": "~4.7.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "maxWorkers": 7
  },
  "yoshi": {
    "hmr": "auto"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress",
      "artifactId": "metro-common-builders"
    }
  },
  "wallaby": {
    "autoDetect": true
  },
  "falconPackageHash": "cb5847097368641bc0fcda87e9793b92e5108c8cc5440c9c5992df4f"
}