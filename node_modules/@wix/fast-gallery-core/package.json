  },
  "devDependencies": {
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@wix/eslint-config-yoshi": "^6.156.0",
