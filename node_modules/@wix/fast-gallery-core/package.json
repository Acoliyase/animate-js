    "ts-jest": "^27.1.5",
    "typescript": "~4.7.0"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "jest": {
    "preset": "@wix/jest-yoshi-preset"
  },
  "yoshiFlowLibrary": {
    "buildEsmWithBabel": false
