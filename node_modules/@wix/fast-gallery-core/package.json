    "validate:lib": "node scripts/validate-server-safe.js",
    "lint": "yoshi-library lint"
  },
  "lint-staged": {
    "*.{js,ts}": "yarn lint"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.2",
