  "exports": {
    "types": "./dist/types/exports.d.ts",
    "import": "./dist/esm/exports.js",
    "require": "./dist/cjs/exports.js"
  },
  "types": "dist/types/exports.d.ts",
  "main": "dist/cjs/exports.js",
  "module": "dist/esm/exports.js",
  "sideEffects": false,
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/types"
  ],
  "scripts": {
    "build": "rm -rf dist && yarn build:ts && mkdir dist/statics && echo \"{}\" > dist/statics/dummy.json",
    "build:ts": "tsc --declarationDir dist/types && tsc --module CommonJS --outDir dist/cjs",
    "deps-check": "yarn deps-check:wix-sled3-compatibility",
    "deps-check:wix-sled3-compatibility": "node ./scripts/wix-sled3-compatibility-check.js",
    "lint": "eslint ./src --max-warnings=0",
    "lint:fix": "yarn lint --fix",
    "test": "playwright test -c playwright.config.ts",
    "test:ui": "playwright test --ui",
    "ts": "tsc --noEmit"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress",
      "artifactId": "unidriver-playwright"
    },
    "validations": {
      "postDependenciesBuild": [
        "deps-check",
        "lint"
      ],
      "postBuild": [
        "test"
      ]
    }
  },
  "dependencies": {
    "@wix/unidriver-common": "^1.4.7",
    "@wix/unidriver-core": "^1.4.7"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "1.51.0",
