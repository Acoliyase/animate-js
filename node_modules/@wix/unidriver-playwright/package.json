    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/semver": "^7.7.1",
    "@wix/playwright": "^1.95.0",
    "ci-info": "^4.3.1",
    "eslint": "^9.37.0",
    "playwright": "1.51.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semver": "^7.7.2",
    "storybook": "^9.1.10",
    "typescript": "^5.9.3"
  },
