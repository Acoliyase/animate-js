      "postDependenciesBuild": [
        "lint"
      ],
      "postBuild": [
        "test"
      ]
    }
  },
  "wallaby": {
    "autoDetect": true
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
